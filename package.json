{
  "name": "codeagent",
  "displayName": "CodeAgent",
  "description": "A quick, efficient and local coding agent!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.addComment",
        "title": "Add Comment"
      },
      {
        "command": "extension.describeCode",
        "title": "Describe Selected Code"
      },
      {
        "command": "extension.debugCode",
        "title": "Debug Selected Code"
      },
      {
        "command": "extension.generateDocument",
        "title": "Generate Document"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "marked": "^15.0.4"
  }
}
